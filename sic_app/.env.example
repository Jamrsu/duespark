# DueSpark Frontend Environment Configuration Template
# ⚠️  SECURITY WARNING: Never commit real secrets to version control
# Copy this file to .env and fill in your actual values

# API Configuration
VITE_API_URL=http://localhost:8005
VITE_API_V1_STR=/api/v1

# Frontend Configuration
VITE_APP_NAME=DueSpark
VITE_APP_VERSION=0.1.0

# Environment
NODE_ENV=development

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_DATA=false

# External Services (if needed for frontend)
# VITE_STRIPE_PUBLIC_KEY=pk_test_XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
# VITE_SENTRY_DSN=https://xxxxxx@sentry.io/xxxxxx

# Development Tools
VITE_DISABLE_SSL=false
VITE_PORT=5173